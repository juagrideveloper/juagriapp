import kotlin.Boolean;

CREATE TABLE Table1 (
    id INTEGER NOT NULL PRIMARY KEY,
    title TEXT NOT NULL
);

getTable1:
SELECT title FROM Table1;

insertTable1:
    INSERT OR REPLACE INTO Table1(id, title)
    VALUES(?, ?);

deleteTable1:
DELETE FROM Table1;

updateTable1:
UPDATE Table1
SET title = :title
WHERE id = :id;